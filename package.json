{
  "name": "menofhunger-api",
  "version": "0.1.0",
  "private": true,
  "description": "Men of Hunger API (NestJS + Prisma) intended for consumption by a Next.js app.",
  "type": "commonjs",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "nest start --watch",
    "dev:check": "node scripts/check-port.mjs",
    "dev:kill": "node scripts/kill-port.mjs",
    "dev:clean": "npm run dev:kill && npm run dev",
    "start": "node dist/main.js",
    "build": "nest build",
    "build:typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "seed:prod": "node scripts/seed-prod.js",
    "seed:dev": "node scripts/seed-dev.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:create": "node scripts/create-migration-from-db.js",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.12",
    "@prisma/client": "^6.19.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.7",
    "@types/web-push": "^3.6.4",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "helmet": "^7.2.0",
    "image-size": "^2.0.2",
    "prisma": "^6.19.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.12.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.11",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "fast-xml-parser": "5.3.4"
  }
}
